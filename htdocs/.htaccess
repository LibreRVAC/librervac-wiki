Options -Indexes

RewriteEngine On

RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://progarm.org/$1 [R,L]

RewriteCond %{HTTP_HOST} ^www.(.+)$ [NC]
RewriteRule ^(.*)$ https://%1/$1 [R=301,L]

RewriteCond %{QUERY_STRING} ^$
RewriteRule ^private/([^/]*)$ /private/cgi-bin/wiki.pl/$1 [QSA,L]
RewriteRule ^private$ /private/cgi-bin/wiki.pl [QSA,L,PT]

#RewriteCond %{QUERY_STRING} ^$
RewriteRule ^([^/]+)$ /cgi-bin/wiki.pl/$1 [QSA,L]
RewriteRule ^$ cgi-bin/wiki.pl [QSA,L,PT]
